    </recordCreates>
    <recordCreates>
        <description>Create the first three records</description>
        <name>Create_Schedule_records_1_3</name>
        <label>Create Schedule records 1-3</label>
        <locationX>578</locationX>
        <locationY>863</locationY>
        <connector>
            <targetReference>Create_fourth_schedule_record</targetReference>
        </connector>
        <inputReference>varScheduleRecordMultiple</inputReference>
    </recordCreates>
    <recordLookups>
        <description>This object holds the dates that the weekend schedules are set for. Uses the start/end date of the parent offering, then adds number of days in the metadata to get the correct weekend. Example Start Date + 25 days is the Friday when the 1st clinical weekend starts.</description>
        <name>Get_Clinical_Instruction_Metadata</name>
        <label>Get Clinical Instruction Metadata</label>
        <locationX>314</locationX>
        <locationY>863</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Add_Schedule_Records_to_Single_Variable</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Label</field>
            <operator>Contains</operator>
            <value>
                <elementReference>varCounter</elementReference>
            </value>
        </filters>
        <object>Clinical_Instruction__mdt</object>
        <outputAssignments>
            <assignToReference>varSartOffset</assignToReference>
            <field>Weekend_Begins__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>varEndOffset</assignToReference>
            <field>Weekend_Ends__c</field>
        </outputAssignments>
    </recordLookups>
    <start>
        <locationX>122</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Counter</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Course_Offering_Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Hybrid Clinical</stringValue>
            </value>
        </filters>
        <object>hed__Course_Offering__c</object>
        <recordTriggerType>Create</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
