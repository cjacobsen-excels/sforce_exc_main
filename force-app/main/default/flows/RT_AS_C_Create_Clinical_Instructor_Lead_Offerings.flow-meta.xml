    <assignments>
        <description>Set the value of the counter to create correct number of Lead offerings</description>
        <name>Counter</name>
        <label>Counter</label>
        <locationX>248</locationX>
        <locationY>323</locationY>
        <assignmentItems>
            <assignToReference>varCounter</assignToReference>
            <operator>Assign</operator>
            <value>
                <numberValue>1.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Check_Counter</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Update the counter before checking checking to create next offering (1-4)</description>
        <name>Counter_add</name>
        <label>Counter ++</label>
        <locationX>50</locationX>
        <locationY>647</locationY>
        <assignmentItems>
            <assignToReference>varCounter</assignToReference>
            <operator>Add</operator>
            <value>
                <numberValue>1.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <isGoTo>true</isGoTo>
            <targetReference>Check_Counter</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Update the counter before checking checking to create next schedule (1-3)</description>
        <name>Next_Counter</name>
        <label>Next Counter ++</label>
        <locationX>314</locationX>
        <locationY>1079</locationY>
        <assignmentItems>
            <assignToReference>varCounter</assignToReference>
            <operator>Add</operator>
            <value>
                <numberValue>1.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <isGoTo>true</isGoTo>
            <targetReference>Check_Counter_for_CO_Schedule_records</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Reset the counter back to one to create the weekend course offering schedules. Three have the same information, the fourth has new values and will be outside of the &quot;loop&quot;</description>
        <name>Reset_Counter</name>
        <label>Reset Counter</label>
        <locationX>446</locationX>
        <locationY>647</locationY>
        <assignmentItems>
            <assignToReference>varCounter</assignToReference>
            <operator>Assign</operator>
            <value>
                <numberValue>1.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Check_Counter_for_CO_Schedule_records</targetReference>
        </connector>
    </assignments>
    <decisions>
        <description>Check the value of the counter to determine if more lead offerings need to be created</description>
        <name>Check_Counter</name>
        <label>Check Counter</label>
        <locationX>248</locationX>
        <locationY>431</locationY>
        <defaultConnector>
            <targetReference>Create_Lead_Offering_Records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Counter equals four</defaultConnectorLabel>
        <rules>
            <name>Less_than_four</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varCounter</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>4.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Lead_Offerings_to</targetReference>
            </connector>
            <label>Less than four</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check the value of the counter to determine if more schedule records need to be created</description>
        <name>Check_Counter_for_CO_Schedule_records</name>
        <label>Check Counter for CO Schedule records</label>
        <locationX>446</locationX>
        <locationY>755</locationY>
        <defaultConnector>
            <targetReference>Create_Schedule_records_1_3</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Counter equals three</defaultConnectorLabel>
        <rules>
            <name>Less_Equal_3</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varCounter</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>3.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Clinical_Instruction_Metadata</targetReference>
            </connector>
            <label>Less/Equal 3</label>
        </rules>
    </decisions>
    <description>Creates the four clinical instructor lead offerings when the Hybrid Clinical Offering is created via integration</description>
    <environments>Default</environments>
    <formulas>
        <description>Assigns end date of offering + offset days from metadata to this variable</description>
        <name>forScheduleEndDate</name>
        <dataType>Date</dataType>
        <expression>{!$Record.hed__Start_Date__c}+{!varEndOffset}</expression>
    </formulas>
    <formulas>
        <description>Assigns start date of offering + offset days from metadata to this variable</description>
        <name>forScheduleStartDate</name>
        <dataType>Date</dataType>
        <expression>{!$Record.hed__Start_Date__c}+{!varSartOffset}</expression>
    </formulas>
    <formulas>
        <description>Holds the name of the schedule weekend</description>
        <name>forWeekendName</name>
        <dataType>String</dataType>
        <expression>&quot;Weekend &quot;&amp;TEXT({!varCounter})</expression>
    </formulas>
    <formulas>
        <description>Holds the name of the Lead offering appended with the counter value (1-4)</description>
        <name>varCourseOfferingLeadName</name>
        <dataType>String</dataType>
        <expression>{!$Record.hed__Course__r.Name}&amp;&quot; CI-&quot;&amp;TEXT({!varCounter})</expression>
    </formulas>
    <interviewLabel>RT-AS-C-Create Clinical Instructor Lead Offerings {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RT-AS-C-Create Clinical Instructor Lead Offerings</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
