    <formulas>
        <description>Concatenates the PoI field for Guild and ArmyIgnitED</description>
        <name>GuildArmyLeadPoI</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record.Area_of_Interest__c}) + &apos; - &apos;+ {!Get_Application_Term.Name}</expression>
    </formulas>
    <formulas>
        <description>Concatenates the PoI values for a lead</description>
        <name>LeadFieldsConcatPoI</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record.Area_of_Interest__c}) + &apos; - &apos;+ UPPER(TEXT({!$Record.Start_Term__c})) + &apos; - &apos; + TEXT({!$Record.Term_Year__c})</expression>
    </formulas>
