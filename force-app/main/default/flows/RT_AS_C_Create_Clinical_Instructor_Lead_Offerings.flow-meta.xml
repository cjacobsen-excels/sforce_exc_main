    <recordCreates>
        <description>Create the lead offerings found in the collection variable. Should be four at the time of this writing.</description>
        <name>Create_Lead_Offering_Records</name>
        <label>Create Lead Offering Records</label>
        <locationX>446</locationX>
        <locationY>539</locationY>
        <connector>
            <targetReference>Reset_Counter</targetReference>
