<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <description>Trigger an Autolaunched flow for Degree progression Task creation that will be assigned to Admissions</description>
    <environments>Default</environments>
    <interviewLabel>RT-CU-AS Degree program Degree Progression Task {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RT-CU-AS Degree program Degree Progression Task</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Call_Subflow</targetReference>
        </connector>
        <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
        <filterLogic>(1 OR 2 OR 3) AND (4 OR 5) AND 6 AND 7 AND 8</filterLogic>
        <filters>
            <field>Service_Program_Code__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>UG</stringValue>
            </value>
        </filters>
        <filters>
            <field>Service_Program_Code__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>GL</stringValue>
            </value>
        </filters>
        <filters>
            <field>Service_Program_Code__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>CT</stringValue>
            </value>
        </filters>
        <filters>
            <field>Degree_Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>A</stringValue>
            </value>
        </filters>
        <filters>
            <field>Degree_Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>AA</stringValue>
            </value>
        </filters>
        <filters>
            <field>School__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>School of Nursing</stringValue>
            </value>
        </filters>
        <filters>
            <field>Student_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Specialist Approved</stringValue>
            </value>
        </filters>
        <filters>
            <field>Academic_Advisor__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>Degree_Program__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <subflows>
        <name>Call_Subflow</name>
        <label>Call Subflow</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <flowName>SSC_Task_Flow</flowName>
        <inputAssignments>
            <name>vContactID</name>
            <value>
                <elementReference>$Record.Student_Name__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>vOwnerID</name>
            <value>
                <elementReference>$Record.Academic_Advisor__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>vTaskName</name>
            <value>
                <stringValue>DP.DPO</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>vWhatId</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
    </subflows>
</Flow>
